{
  "extends": [
    "airbnb",
    "plugin:@typescript-eslint/recommended",
    "airbnb-typescript",
    "next/core-web-vitals",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "parserOptions": {
    "project": "tsconfig.json"
  },
